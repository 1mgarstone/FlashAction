run = "cd ArbitrageEngine-main && npm install && npm run dev"
modules = ["nodejs-20", "web", "postgresql-16"]
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist", "attached_assets"]

[packager]
language = "nodejs-20"

[packager.features]
guessImports = true
packageSearch = true
enabledForHosting = true

[nix]
channel = "stable-24_05"

[deployment]
deploymentTarget = "autoscale"
build = ["sh", "-c", "cd ArbitrageEngine-main && npm install && npm run build"]
run = ["sh", "-c", "cd ArbitrageEngine-main && npm start"]

[env]
NODE_ENV = "production"
PORT = "5000"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 5001

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 19000
externalPort = 3000

[workflows]
runButton = "Gentle Mode"

[[workflows.workflow]]
name = "Start Application"
author = "agent"
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd ArbitrageEngine-main && npm install && npm run dev"
waitForPort = 5000

