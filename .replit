
run = "cd ArbitrageEngine-main && npm install && npm run dev"
modules = ["nodejs-20"]

[packager]
language = "nodejs-20"

[packager.features]
guessImports = true
packageSearch = true
enabledForHosting = true

[nix]
channel = "stable-23_11"

[deployment]
build = ["sh", "-c", "cd ArbitrageEngine-main && npm install && npm run build"]
run = ["sh", "-c", "cd ArbitrageEngine-main && npm start"]

[env]
NODE_ENV = "production"
PORT = "5000"

[workflows]
runButton = "Start App"

[[workflows.workflow]]
name = "Start Application"
author = 44850244
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd ArbitrageEngine-main && npm install && npm run dev"

[[workflows.workflow]]
name = "Deploy Maximum Gain Bot"
author = 44850244
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd ArbitrageEngine-main"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "chmod +x full_setup.sh"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./full_setup.sh"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run build"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run maxgain"

[[workflows.workflow]]
name = "System Audit"
author = 44850244
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd ArbitrageEngine-main"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node scripts/auditAndDebug.js"

[[workflows.workflow]]
name = "Fix & Deploy"
author = 44850244
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd ArbitrageEngine-main"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node scripts/fixSystem.js"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run setup"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run build"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"

[[workflows.workflow]]
name = "Start Flash Loan System"
author = 44850244
mode = "sequential"

